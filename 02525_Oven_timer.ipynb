   "source": [
    "h,m = map(int,input().split())\n",
    "t = int(input())\n",
    "if m+t < 60:\n",
    "    m += t\n",
    "else:\n",
    "    k = (m+t)//60\n",
    "    m = (m+t)%60\n",
    "    if h+k < 24:\n",
    "        h += k\n",
    "    else:\n",
    "        h = h+k-24\n",
    "print(h,m)"
   ]
  }
